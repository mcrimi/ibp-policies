Type.registerNamespace('ArcherTech.UI');ArcherTech.UI.HelpManager=function(){ArcherTech.UI.HelpManager.initializeBase(this);this._helpCenterPath=null;};ArcherTech.UI.HelpManager.prototype={initialize:function(){},showHelp:function(helpId){var win=window.top,archerHelp=(win.Help)?win.Help:null,dHelpId=(archerHelp.cshId.DEFAULT)?archerHelp.cshId.DEFAULT:10000;if(archerHelp){if(helpId){archerHelp.show(helpId);}else{archerHelp.show(dHelpId);}}else{console.log('Unable to open Help Window.');}},showTopicUrl:function(topicUrl){var win=window.top,archerHelp=(win.Help)?win.Help:null;if(archerHelp){if(topicUrl){archerHelp.showTopicUrl(topicUrl);}else{console.log('Unable to open Topic Window. URL is missing.');}}else{console.log('Unable to open Topic Window.');}},setHelpOnMegaMenu:function(helpId){var win=window.top,archerHelp=(win.Help)?win.Help:null,dHelpId;if(archerHelp){dHelpId=(archerHelp.cshId.DEFAULT)?archerHelp.cshId.DEFAULT:10000;if(helpId){archerHelp.setHelpCshId(helpId);}else{archerHelp.setHelpCshId(dHelpId);}}else{console.log('Unable to set Topic Id for Mega Menu.');}}};ArcherTech.UI.HelpManager.registerClass('ArcherTech.UI.HelpManager',Sys.Component);if(typeof(Sys)!=='undefined')Sys.Application.notifyScriptLoaded();